<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ticket.app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_tickets"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_tickets"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ticket.app.SignUpActivity"></activity>
        <activity android:name="com.ticket.app.ProfileActivity" />
        <activity android:name="com.ticket.app.LoginActivity" />
        <activity android:name="com.ticket.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>